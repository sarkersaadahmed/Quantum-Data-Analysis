SELECT TOP (1000) [Entity]
      ,[Year]
      ,[Electricity from nuclear - TWh]
      ,[Code]
  FROM [portfolio_on_nuclear_energy_generator].[dbo].['nuclear-energy-generation$']

SELECT 
    Entity,  
    [Electricity from nuclear - TWh],
    [Electricity from nuclear - TWh] * 1000 as [Electricity from nuclear - MWh],
    Year,
    MAX([Electricity from nuclear - TWh]) as highest_electricity_generated,
    Code
FROM 
    portfolio_on_nuclear_energy_generator.dbo.['nuclear-energy-generation$']
WHERE 
    Entity LIKE '%Asia%'
GROUP BY
	Entity, [Electricity from nuclear - TWh], Year, Code
ORDER BY 
    Year DESC;
